$requestList = $(".request-list")
if ($requestList.length > 0) {
  $requestList.html("<%= escape_javascript render(:partial => 'shared/request_list', :object => @messages) %>");
  registerEventForRequestActionBtns();
}

$requestAccept = $(".<%= dom_id @request %> .request-action-accept")
$requestCancel = $(".<%= dom_id @request %> .request-action-cancel")
if ($requestAccept.length > 0 && $requestCancel.length > 0) {
  $requestAccept.toggle(300);
  $requestCancel.toggle(300);
}
